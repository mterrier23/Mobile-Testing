<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PetInsights_all.MainProfile"
			 IconImageSource="ic_person_outline.png"
			 Title="Profile"
             BackgroundColor="white">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="User Page" TextColor="Black" HorizontalOptions="Center" VerticalOptions="CenterAndExpand" />
            <Entry Placeholder ="Username" StyleId="username"/>
            <Entry Placeholder ="Password" StyleId="password"/>
            <Button Text="Login" StyleId="loginButton" BackgroundColor="Blue" Clicked="LoginButton_clicked" VerticalOptions="CenterAndExpand">
            </Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>